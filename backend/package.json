{
  "name": "graphql-apollo-server-github-scanner-backend",
  "version": "1.0.0",
  "private": true,
  "description": "GraphQL Apollo Server GitHub Scanner Backend",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .ts src __test__",
    "lint:fix": "eslint --fix --ext .ts src __test__",
    "codegen": "graphql-codegen",
    "test": "jest",
    "start:dev": "ts-node -r dotenv/config src/main.ts",
    "build:src:drop": "rimraf ./dist",
    "build:src:clean": "tsc --build tsconfig.build.json --clean && rm -rf ./dist",
    "build:src:make": "tsc --build tsconfig.build.json",
    "build:src": "run-s build:src:*",
    "start:build": "node -r dotenv/config dist/main.js"
  },
  "keywords": [
    "github",
    "graphql",
    "scanning"
  ],
  "author": "Dzianis Buyakou <dzianis.buyakou@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "async": "^3.2.4",
    "async-retry": "^1.3.3",
    "axios": "^1.5.0",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "node-fetch": "^3.3.2",
    "octokit": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@octokit/types": "^12.0.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/async": "^3.2.21",
    "@types/async-retry": "^1.4.6",
    "@types/jest": "^26.0.24",
    "jest": "^27.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.10.1",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-class-members": "^1.15.2",
    "prettier": "^2.3.2"
  }
}
